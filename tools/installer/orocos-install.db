# Install data used by Orocos installer
# Adapted from ecos-install.db by Peter Soetens.

# Install data for v1 format - allows for changes in future installers.

namespace eval installdatav1 {
        # releases list. Array of n-tuples of dirname, directory name relative to base, appropriate ECOS_REPOSITORY setting, list of elements relative
        # to base to add to PATH.
        # First one listed should be default.

        array set ecosreleases \
            { \
                  "Linux" { \
                                { "0.19.3" "packages/0.19.3/orocos-0.19.3.i386linux.tar.bz2" "orocos-0.19.3/packages" { "orocos-0.19.3/tools/bin"} } \
                                { "0.19.2" "packages/0.19.2/orocos-0.19.2.i386linux.tar.bz2" "orocos-0.19.2/packages" { "orocos-0.19.2/tools/bin"} } \
                                { "0.19.1" "packages/0.19.1/orocos-0.19.1.i386linux.tar.bz2" "orocos-0.19.1/packages" { "orocos-0.19.1/tools/bin"} } \
                                { "0.19.0" "packages/0.19.0/orocos-0.19.0.i386linux.tar.bz2" "orocos-0.19.0/packages" { "orocos-0.19.0/tools/bin"} } \
                                { "0.18.2" "packages/0.18.2/orocos-0.18.2.i386linux.tar.bz2" "orocos-0.18.2/packages" { "orocos-0.18.2/tools/bin"} } \
                                { "0.18.1" "packages/0.18.1/orocos-0.18.1.i386linux.tar.bz2" "orocos-0.18.1/packages" { "orocos-0.18.1/tools/bin"} } \
                                { "0.18.0" "packages/0.18.0/orocos-0.18.0.i386linux.tar.bz2" "orocos-0.18.0/packages" { "orocos-0.18.0/tools/bin"} } \
                                { "0.16.0" "packages/0.16.0/orocos-0.16.0.i386linux.tar.bz2" "orocos-0.16.0/packages" { "orocos-0.16.0/tools/bin"} } \
                                { "0.14.2" "packages/0.14.2/orocos-0.14.2.i386linux.tar.bz2" "orocos-0.14.2/packages" { "orocos-0.14.2/tools/bin"} } \
                                { "0.14.0" "packages/0.14.0/orocos-0.14.0.i386linux.tar.bz2" "orocos-0.14.0/packages" { "orocos-0.14.0/tools/bin"} } \
                                { "0.12.0" "packages/0.12.0/orocos-0.12.0.i386linux.tar.bz2" "orocos-0.12.0/packages" { "orocos-0.12.0/tools/bin"} } \
                                { "0.10.0" "packages/0.10.0/orocos-0.10.0.i386linux.tar.bz2" "orocos-0.10.0/packages" { "orocos-0.10.0/tools/bin"} } \
                        } \
          }

        # list of available toolchains. Prefix and location relative to base. Then path suffix for what to add to PATH
        array set toolchains \
            { \
                  "Linux" { \
                                { "arm-elf"       "gnutools/i386linux/ecoscentric-gnutools-arm-elf-1.4-2.i386linux.tar.bz2"        "gnutools/arm-elf/bin" } \
                                { "i386-elf"      "gnutools/i386linux/ecoscentric-gnutools-i386-elf-1.4-2.i386linux.tar.bz2"       "gnutools/i386-elf/bin" } \
                                { "mipsisa32-elf" "gnutools/i386linux/ecoscentric-gnutools-mipsisa32-elf-1.4-2.i386linux.tar.bz2"  "gnutools/mipsisa32-elf/bin" } \
                                { "powerpc-eabi"  "gnutools/i386linux/ecoscentric-gnutools-powerpc-eabi-1.4-2.i386linux.tar.bz2"   "gnutools/powerpc-eabi/bin" } \
                                { "sh-elf"        "gnutools/i386linux/ecoscentric-gnutools-sh-elf-1.4-2.i386linux.tar.bz2"         "gnutools/sh-elf/bin" } \
                        } \
          }
        
        variable ecosmirrors [ list \
				   "http://people.mech.kuleuven.ac.be/~psoetens/orocos/" \
                                  ]
}

if { [get_install_data_version] == 1 } {
        set_ecosreleases [array get installdatav1::ecosreleases]
        set_toolchains [array get installdatav1::toolchains]
        set_ecosmirrors $installdatav1::ecosmirrors
}

# EOF orocos-install.db
