<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "cpf.dtd">

<properties>
    <struct name="KernelProperties" type="type_less"> <description>The Basic characteristics of this kernel.</description>
       <simple name="frequency" type="double"><description>The periodic execution frequency in Hz</description><value>100</value></simple>
    </struct>

    <struct name="StartupComponents" type="type_less">  <description>The Components to be Selected when the kernel is started</description>
       <simple name="Sensor"     type="string"> <value>nAxesSensorPos</value> </simple>
       <simple name="Controller" type="string"> <value>nAxesControllerPos</value> </simple>
<!--       <simple name="Controller" type="string"> <value>nAxesControllerVel</value> </simple>-->
<!--       <simple name="Controller" type="string"> <value>nAxesControllerPosVel</value> </simple> -->
       <simple name="Generator"  type="string"> <value>nAxesGeneratorPos</value> </simple>
<!--       <simple name="Generator"  type="string"> <value>nAxesGeneratorVel</value> </simple> -->
       <simple name="Effector"   type="string"> <value>nAxesEffectorVel</value> </simple>
    </struct>

    <struct name="Extensions" type="type_less">
       <simple name="Reporter" type="string">  <description>The property file of the reporter</description>         <value>cpf/reporter.cpf</value></simple>
       <simple name="Property" type="string">  <description>The property file of the property manager</description> <value>cpf/property.cpf</value></simple>
    </struct>
</properties>


