<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "cpf.dtd">
<!-- Properties of the Position-Generator component -->
<properties>
   <simple name="Name" type="string"><description>The unique name of this component</description><value>nAxesPosGenerator</value></simple>
