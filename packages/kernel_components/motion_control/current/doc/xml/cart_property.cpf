<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "cpf.dtd">
<!-- Properties of the PropertyExtension -->
<properties>

    <simple name="Generator"  type="string"><description>The Generator Component Property File.</description><value>cart_generator.cpf</value></simple>
    <simple name="Estimator"  type="string"><description>The Estimator Component Property File.</description><value>cart_estimator.cpf</value></simple>
    <simple name="Controller" type="string"><description>The Controller Component Property File.</description><value>cart_controller.cpf</value></simple>
    <simple name="Sensor"     type="string"><description>The Sensor Component Property File.</description><value>cart_sensor.cpf</value></simple>
    <simple name="Effector"   type="string"><description>The Effector Component Property File.</description><value>cart_effector.cpf</value></simple>

    <simple name="SaveProperties" type="boolean">
        <description> True if properties of components need to be saved when stopping the kernel.</description>
        <value>1</value>
    </simple>
    <simple name="SaveFilePrefix"   type="string">
        <description>The suffix of the filename to store the components state into</description><value>saved_</value>
    </simple>
    <simple name="SaveFileExtension"   type="string">
        <description>The extension of the filename to store the components state into</description><value>cpf</value>
    </simple>
    
</properties>
