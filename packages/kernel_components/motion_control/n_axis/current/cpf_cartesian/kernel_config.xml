<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "cpf.dtd">

<properties>
    <struct name="KernelProperties" type="type_less"> <description>The Basic characteristics of this kernel.</description>
       <simple name="frequency" type="double">
          <description>The periodic execution frequency in Hz</description>
          <value>100</value></simple>
    </struct>
    <struct name="StartupComponents" type="type_less">  
       <description>The Components to be Selected when the kernel is started</description>
       <simple name="Sensor"     type="string"> <value>nAxesSensorCartesianPos</value> </simple>
<!--       <simple name="Controller" type="string"> <value>nAxesControllerCartesianPos</value> </simple>-->
       <simple name="Controller" type="string"> <value>nAxesControllerCartesianVel</value> </simple>
<!--       <simple name="Controller" type="string"> <value>nAxesControllerCartesianPosVel</value> </simple>-->
       <simple name="Generator"  type="string"> <value>nAxesGeneratorCartesianPos</value> </simple>
       <simple name="Effector"   type="string"> <value>nAxesEffectorCartesianVel</value> </simple>
    </struct>

    <struct name="Extensions" type="type_less">
       <simple name="Reporter" type="string">  
              <description>The property file of the reporter</description>         
              <value>cpf_cartesian/reporter.cpf</value></simple>
       <simple name="Property" type="string">  
              <description>The property file of the property manager</description> 
              <value>cpf_cartesian/property.cpf</value></simple>
    </struct>
</properties>


