<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "cpf.dtd">
<!-- Properties of the PropertyExtension -->
<properties>
   <sequence name="PropertyFiles" type="ComponentEntry">
      <simple name="nAxesVelController" type="string"> <description>The Property File.</description> <value>cpf/nAxesControllerVel.cpf</value> </simple>  
      <simple name="nAxesPosController" type="string"> <description>The Property File.</description> <value>cpf/nAxesControllerPos.cpf</value> </simple>  
      <simple name="nAxesPosVelController" type="string"> <description>The Property File.</description> <value>cpf/nAxesControllerPosVel.cpf</value> </simple>  
      <simple name="nAxesPosGenerator"  type="string"> <description>The Property File.</description> <value>cpf/nAxesGeneratorPos.cpf</value> </simple>  
      <simple name="nAxesVelGenerator"  type="string"> <description>The Property File.</description> <value>cpf/nAxesGeneratorSin.cpf</value> </simple>  
   </sequence>
   <simple name="SaveProperties" type="boolean"> <description> True if properties of components needs saved when stopping kernel.</description><value>true</value> </simple>
   <simple name="SaveFilePrefix"   type="string">   <description>The suffix of the filename to store the components state into</description><value>saved_</value> </simple>
   <simple name="SaveFileExtension"   type="string"> <description>The extension of filename to store the components state into</description><value>cpf</value> </simple>
</properties>
