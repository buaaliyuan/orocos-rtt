cdl_package OROPKG_SUPPORT_COMEDI {
    display "Support Comedi installation"
    parent OROPKG_SUPPORT

    description " 
The presence of this package indicates that the configure
script detected a valid Comedi installation, which can now
be used by other packages."
    cdl_option OROBLD_SUPPORT_COMEDI_DIR {
	display "Comedi installation directory"
	flavor data
	default_value {"@COMEDI_DIR@"}
    }
}